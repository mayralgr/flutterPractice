  final wolverine = new Heroe('Logan', 'regeneraciÃ³n');
  print(wolverine);
  
}

class Heroe{
  
  String nombre;
  String poder;
  
  Heroe(String nombre, String poder)
  {
    this.nombre = nombre;
    this.poder = poder;
  }
  
  String toString()=> 'nombre: $nombre - poder: $poder';
  
}